{
    "configurations": [
        {
            "name": "MadOS",
            "includePath": [
                "${env.MADOS_TOOLS_HOME}/arm-none-eabi/include",
                "${env.MADOS_TOOLS_HOME}/lib/gcc/arm-none-eabi/${env.MADOS_TOOLS_VER}/include",
                "${workspaceFolder}/arch/stm32f10x/Arch",
                "${workspaceFolder}/arch/stm32f10x/Startup",
                "${workspaceFolder}/arch/stm32f10x/StdPeriph",
                "${workspaceFolder}/arch/stm32f10x/StdPeriph/inc",
                "${workspaceFolder}/library/Newlib",
                "${workspaceFolder}/driver",
                "${workspaceFolder}/driver/stm32f10x",
                "${workspaceFolder}/kernel/inc",
                "${workspaceFolder}/kernel/lib/pt",
                "${workspaceFolder}/kernel/lib/timer",
                "${workspaceFolder}/library/uIP/uip",
                "${workspaceFolder}/library/uIP/uip-funs",
                "${workspaceFolder}/library/FatFs",
                "${workspaceFolder}/library/cJSON",
                "${workspaceFolder}/library/Lua",
                "${workspaceFolder}/app/test_module"
            ],
            "defines": [
                "MALLOC_PROVIDED",
                "MISSING_SYSCALL_NAMES",
                "REENTRANT_SYSCALLS_PROVIDED",
                "USE_STDPERIPH_DRIVER",
                "STM32F10X_CL"
            ],
            "compilerPath": "${env.MADOS_TOOLS_HOME}/bin/arm-none-eabi-gcc",
            "cStandard": "c11",
            "cppStandard": "c++17"
        }
    ],
    "version": 4
}
